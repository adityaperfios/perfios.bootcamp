package com.perfs;
import com.perfs.Modifiers;

public class Class2 {
	
	public void disp(Modifiers obj)
	{
		System.out.println("Accessing public var (a) of class modifiers "
				+ "through Class2 "+obj.a);
	}
	public static void main(String[] args) {
		Modifiers = obj = new Modifiers();
		obj.disp();
		
	}
}

class Class3 extends Modifiers
{	
	Modifiers obj = new Modifiers();
	
	public void disp() {
	System.out.println("accessing default var (y)"+obj.y);
	System.out.println("accessing protected var (z)"+ obj.z);
}
	}
