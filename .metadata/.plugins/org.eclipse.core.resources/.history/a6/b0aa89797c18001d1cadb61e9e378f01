package pkg;
class Demo{
	public int roll;
	public String name;
	public Demo(int a,String n)
	{
		name = n;
		roll = a;
		
	}
	public void disp()
	{
		System.out.println("Name = "+ name);
		System.out.println("roll no = "+ roll);
	}
}
public class Student {
	public static void main(String[] args) {
		Demo obj = new Demo(12, "Dilip");
	}
}
