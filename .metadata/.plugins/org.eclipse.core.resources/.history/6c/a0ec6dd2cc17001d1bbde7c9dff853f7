package com.perfs;

public class Modifiers {
	private int x = 100;
	 		int y = 30;
	protected int z = 44;
	public int a = 22;
	
	public void disp() {
		System.out.println("in Modifiers class "+x);
		System.out.println("in Modifiers class "+y);
		System.out.println("in Modifiers class "+z);
		System.out.println("in Modifiers class "+a);
	}
}
